{
    "name": "toolstud-io/nowtricity",
    "description": "PHP Library for Nowtricity API",
    "keywords": [
        "toolstud-io",
        "nowtricity"
    ],
    "homepage": "https://github.com/toolstud-io/nowtricity",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Forret",
            "email": "peter@forret.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-curl": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "ToolstudIo\\Nowtricity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ToolstudIo\\Nowtricity\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint",
        "test": "vendor/bin/phpunit --testdox",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.0.0"
}
